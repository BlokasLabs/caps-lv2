@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .


<http://portalmod.com/plugins/caps/Clip> a lv2:Plugin, lv2:DistortionPlugin;
    doap:name  "C* Clip - Hard clipper, 8x oversampled";
    mod:label "C* Clip";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 3;
   
    mod:pedalModel "japanese";
    mod:pedalColor "orange";
    mod:brand "caps";
    mod:pedalLabel "Hard Clipper";
    mod:smallLabel "Clip";
    mod:bypassLedPosition "118-83";


    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "gain";
        lv2:name "gain";
        lv2:default 9.2;
        lv2:minimum -72;
        lv2:maximum 72;
        mod:pedalButton "1-3";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 2;
        lv2:symbol "out";
        lv2:name "out";
    ],
    [
        a lv2:ControlPort, lv2:OutputPort;
        lv2:index 3;
        lv2:symbol "latency";
        lv2:name "latency";
        lv2:default 1.0;
        lv2:minimum -72;
        lv2:maximum 72;
    ].
