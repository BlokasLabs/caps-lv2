@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<http://portalmod.com/plugins/caps/Spicex2> a lv2:Plugin, lv2:DistortionPlugin;
    doap:name  "C* Spicex2";

    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];

    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];

    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;

    lv2:minorVersion 9;
    lv2:microVersion 16;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "lof";
        lv2:name "lo.f";
        lv2:default 225.0;
        lv2:minimum 50.0;
        lv2:maximum 800.0;
        lv2:portProperty epp:logarithmic;
        units:unit units:Hz;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "locomp";
        lv2:name "lo.compress";
        lv2:default 0.5;
        lv2:minimum 0.0;
        lv2:maximum 1.0;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "logain";
        lv2:name "lo.gain";
        lv2:default 0.25;
        lv2:minimum 0.0;
        lv2:maximum 1.0;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "hif";
        lv2:name "hi.f";
        lv2:default 1350.0;
        lv2:minimum 400.0;
        lv2:maximum 5000.0;
        lv2:portProperty epp:logarithmic;
        units:unit units:Hz;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "higain";
        lv2:name "hi.gain";
        lv2:default 0.25;
        lv2:minimum 0.0;
        lv2:maximum 1.0;
    ],
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "inl";
        lv2:name "in:l";
    ],
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "inr";
        lv2:name "in:r";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 7;
        lv2:symbol "outl";
        lv2:name "out:l";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 8;
        lv2:symbol "outr";
        lv2:name "out:r";
    ].
