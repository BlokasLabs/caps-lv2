@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://portalmod.com/plugins/caps/ToneStack> a lv2:Plugin,
        lv2:SimulatorPlugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0;
            lv2:index 1;
            lv2:maximum 9;
            lv2:minimum 0;
            lv2:name "model";
            lv2:portProperty lv2:enumeration,
                lv2:integer;
            lv2:scalePoint [ rdfs:label "Basswoman";
                    rdf:value 0 ],
                [ rdfs:label "Twin";
                    rdf:value 1 ],
                [ rdfs:label "Wookie";
                    rdf:value 2 ],
                [ rdfs:label "DC 30";
                    rdf:value 3 ],
                [ rdfs:label "Juice 800";
                    rdf:value 4 ],
                [ rdfs:label "Stanford";
                    rdf:value 5 ],
                [ rdfs:label "HK 20";
                    rdf:value 6 ],
                [ rdfs:label "Nihon Ace";
                    rdf:value 7 ],
                [ rdfs:label "Porky";
                    rdf:value 8 ],
                [ rdfs:label "5F6-A LT";
                    rdf:value 9 ];
            lv2:symbol "model" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "bass";
            lv2:symbol "bass" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 3;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "mid";
            lv2:symbol "mid" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 4;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "treble";
            lv2:symbol "treble" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 5;
            lv2:name "out";
            lv2:symbol "out" ];
    mod:gui [ mod:iconTemplate <modgui/ToneStack.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/ToneStack.png>;
            mod:templateData <modgui/ToneStack.json>;
            mod:thumbnail <modgui/ToneStack-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* ToneStack - Tone stack emulation" .

