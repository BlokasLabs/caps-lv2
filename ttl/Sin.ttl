@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://portalmod.com/plugins/caps/Sin> a lv2:OscillatorPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;

rdfs:comment """
The old friend, indispensable for testing and tuning.â€‚

source: http://quitte.de/dsp/caps.html#Sin
""";

    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 100.0;
            lv2:index 0;
            lv2:maximum 20000;
            lv2:minimum 0.00010;
            lv2:name "f";
            lv2:portProperty pprops:logarithmic,
                epp:logarithmic;
            lv2:symbol "f" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 1;
            lv2:maximum 1;
            lv2:minimum 0.0000010;
            lv2:name "volume";
            lv2:symbol "volume" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 2;
            lv2:name "out";
            lv2:symbol "out" ];
    mod:gui [ mod:iconTemplate <modgui/Sin.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/Sin.png>;
            mod:templateData <modgui/Sin.json>;
            mod:thumbnail <modgui/Sin-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* Sin - Sine wave generator" .

