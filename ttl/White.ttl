@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://moddevices.com/ns/modgui#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://moddevices.com/plugins/caps/White> a lv2:GeneratorPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;

rdfs:comment """
Mostly white pseudonoise, mixed and filtered from the output of two Dattorro multibit generators.

source: http://quitte.de/dsp/caps.html#White
""";

    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 0;
            lv2:maximum 1;
            lv2:minimum 0.0000010;
            lv2:name "volume";
            lv2:symbol "volume" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 1;
            lv2:name "out";
            lv2:symbol "out" ];
    mod:gui [ mod:iconTemplate <modgui/White.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/White.png>;
            mod:stylesheet <modgui/_common.css>;
            mod:templateData <modgui/White.json>;
            mod:thumbnail <modgui/White-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://moddevices.com>;
            foaf:mbox <mailto:devel@moddevices.com>;
            foaf:name "MOD Team" ];
    doap:name "C* White - White noise generator" .

