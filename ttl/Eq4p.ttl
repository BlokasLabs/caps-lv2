            units:unit units:hz;
            lv2:default 2721.780;
            lv2:index 13;
            lv2:maximum 14000;
            lv2:minimum 20;
            lv2:name "d.freq";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "d_freq" ],
        [ a lv2:ControlPort,
