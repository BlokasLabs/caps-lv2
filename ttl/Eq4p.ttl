@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/caps/Eq4p> a lv2:Plugin, lv2:EQPlugin;
    doap:name  "C* Eq4p - 4-band parametric equaliser";
    mod:label "C* Eq4p";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "a_mode";
        lv2:name "a.mode";
        lv2:default 0;
        lv2:minimum -1;
        lv2:maximum 2;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "off"; rdf:value -1];
        lv2:scalePoint [rdfs:label "lowshelve"; rdf:value 0];
        lv2:scalePoint [rdfs:label "band"; rdf:value 1];
        lv2:scalePoint [rdfs:label "hishelve"; rdf:value 2];
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "a_freq";
        lv2:name "a.freq";
        units:unit units:hz ;
        lv2:portProperty epp:logarithmic;
        lv2:default 102.87;
        lv2:minimum 20;
        lv2:maximum 14000;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "a_q";
        lv2:name "a.Q";
        lv2:default 0.25;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "a_gain";
        lv2:name "a.gain";
        units:unit units:db ;
        lv2:default 0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "b_mode";
        lv2:name "b.mode";
        lv2:default 1;
