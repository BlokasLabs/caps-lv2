@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://portalmod.com/plugins/caps/Spice> a lv2:DynamicsPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 1;
            lv2:name "out";
            lv2:symbol "out" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 225.0;
            lv2:index 2;
            lv2:maximum 400.0;
            lv2:minimum 50.0;
            lv2:name "lo.f";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "lof" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 3;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "lo.compress";
            lv2:symbol "locomp" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 4;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "lo.gain";
            lv2:symbol "logain" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 2700.0;
            lv2:index 5;
            lv2:maximum 5000.0;
            lv2:minimum 400.0;
            lv2:name "hi.f";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "hif" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 6;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "hi.gain";
            lv2:symbol "higain" ];
    mod:gui [ mod:iconTemplate <modgui/Spice.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/Spice.png>;
            mod:templateData <modgui/Spice.json>;
            mod:thumbnail <modgui/Spice-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* Spice" .

