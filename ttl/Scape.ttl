@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://quitte.de/dsp/caps.html#Scape> a lv2:Plugin, lv2:DelayPlugin;
    doap:name  "C* Scape - Stereo delay + Filters";

    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];

    doap:maintainer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];

    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 3;

    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "bpm";
        lv2:name "bpm";
        lv2:default 97;
        lv2:minimum 30;
        lv2:maximum 164;
        units:unit units:bpm ;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "divider";
        lv2:name "divider";
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "eighths"; rdf:value 2];
        lv2:scalePoint [rdfs:label "triplets"; rdf:value 3];
        lv2:scalePoint [rdfs:label "sixteenths"; rdf:value 4];
        lv2:default 2;
        lv2:minimum 2;
        lv2:maximum 4;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "feedback";
        lv2:name "feedback";
        lv2:default 0.75;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "dry";
        lv2:name "dry";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
	],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "blend";
        lv2:name "blend";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
	],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "tune";
        lv2:name "tune";
        lv2:default 440;
        lv2:minimum 415;
        lv2:maximum 467;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 7;
        lv2:symbol "outl";
        lv2:name "out:l";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 8;
        lv2:symbol "outr";
        lv2:name "out:r";
    ].
