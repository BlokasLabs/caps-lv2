@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://moddevices.com/plugins/caps/SpiceX2> a lv2:DynamicsPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    mod:brand "CAPS";
    mod:label "SpiceX2";

rdfs:comment """
Stereo version of Spice. Bass compression is governed by the sum of both channels, as in CompressX2. Nevertheless, the amount of harmonic generation differing between the two channels can have subtle effects on the stereo image. 

source: http://quitte.de/dsp/caps.html#Spice
""";

    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 225.0;
            lv2:index 0;
            lv2:maximum 800.0;
            lv2:minimum 50.0;
            lv2:name "lo.f";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "lof" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 1;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "lo.compress";
            lv2:symbol "locomp" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 2;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "lo.gain";
            lv2:symbol "logain" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 1350.0;
            lv2:index 3;
            lv2:maximum 5000.0;
            lv2:minimum 400.0;
            lv2:name "hi.f";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "hif" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 4;
            lv2:maximum 1.0;
            lv2:minimum 0.0;
            lv2:name "hi.gain";
            lv2:symbol "higain" ],
        [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 5;
            lv2:name "in:l";
            lv2:symbol "inl" ],
        [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 6;
            lv2:name "in:r";
            lv2:symbol "inr" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 7;
            lv2:name "out:l";
            lv2:symbol "outl" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 8;
            lv2:name "out:r";
            lv2:symbol "outr" ];
    modgui:gui [ modgui:iconTemplate <modgui/SpiceX2.html>;
            modgui:resourcesDirectory <modgui>;
            modgui:screenshot <modgui/SpiceX2.png>;
            modgui:stylesheet <modgui/_common.css>;
            modgui:templateData <modgui/SpiceX2.json>;
            modgui:thumbnail <modgui/SpiceX2-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://moddevices.com>;
            foaf:mbox <mailto:devel@moddevices.com>;
            foaf:name "MOD Team" ];
    doap:name "C* SpiceX2" .

