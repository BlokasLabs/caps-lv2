@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://moddevices.com/plugins/caps/ChorusI> a lv2:ChorusPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;

rdfs:comment """
A standard mono chorus with optional feedback. The parameter range suits subtle effects as well as all-out flanging. 

Modifying the delay time t when feedback is active will cause audible 'zipper' noise. 

source: http://quitte.de/dsp/caps.html#ChorusI
""";

    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:ms;
            lv2:default 11.880;
            lv2:index 1;
            lv2:maximum 40;
            lv2:minimum 2.50;
            lv2:name "time";
            lv2:portProperty epp:logarithmic;
            lv2:symbol "time" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:ms;
            lv2:default 1.0;
            lv2:index 2;
            lv2:maximum 10;
            lv2:minimum 0.50;
            lv2:name "width";
            lv2:symbol "width" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 1.250;
            lv2:index 3;
            lv2:maximum 5;
            lv2:minimum 0;
            lv2:name "rate";
            lv2:symbol "rate" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 1.0;
            lv2:index 4;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "blend";
            lv2:symbol "blend" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 5;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "feedforward";
            lv2:symbol "feedforward" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.0;
            lv2:index 6;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "feedback";
            lv2:symbol "feedback" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 7;
            lv2:name "out";
            lv2:symbol "out" ];
    modgui:gui [ modgui:iconTemplate <modgui/ChorusI.html>;
            modgui:resourcesDirectory <modgui>;
            modgui:stylesheet <modgui/_common.css>;
            modgui:screenshot <modgui/ChorusI.png>;
            modgui:templateData <modgui/ChorusI.json>;
            modgui:thumbnail <modgui/ChorusI-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://moddevices.com>;
            foaf:mbox <mailto:devel@moddevices.com>;
            foaf:name "MOD Team" ];
    doap:name "C* ChorusI - Mono chorus/flanger" .

