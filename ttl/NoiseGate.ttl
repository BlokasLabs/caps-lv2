@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/NoiseGate> a lv2:UtilityPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 1;
            lv2:name "out";
            lv2:symbol "out" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:db;
            lv2:default -45;
            lv2:index 2;
            lv2:maximum 0;
            lv2:minimum -60;
            lv2:name "Open";
            lv2:symbol "open" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:ms;
            lv2:default 0.750;
            lv2:index 3;
            lv2:maximum 3;
            lv2:minimum 0;
            lv2:name "Attack";
            lv2:symbol "attack" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:db;
            lv2:default -67.50;
            lv2:index 4;
            lv2:maximum 0;
            lv2:minimum -90;
            lv2:name "Close";
            lv2:symbol "close" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:hz;
            lv2:default 50;
            lv2:index 5;
            lv2:maximum 100;
            lv2:minimum 0;
            lv2:name "Mains";
            lv2:scalePoint [ rdfs:label "off";
                    rdf:value 0 ],
                [ rdfs:label "global";
                    rdf:value 50 ],
                [ rdfs:label "imperial";
                    rdf:value 60 ];
            lv2:symbol "mains" ];
    mod:gui [ mod:iconTemplate <modgui/NoiseGate.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/NoiseGate.png>;
            mod:templateData <modgui/NoiseGate.json>;
            mod:thumbnail <modgui/NoiseGate-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* NoiseGate - Attenuate noise resident in silence" .

