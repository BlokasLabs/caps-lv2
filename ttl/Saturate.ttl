@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/Saturate> a lv2:DistortionPlugin,
        lv2:Plugin;
    lv2:microVersion 2;
    lv2:minorVersion 0;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 1;
            lv2:name "out";
            lv2:symbol "out" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 1;
            lv2:index 2;
            lv2:maximum 11;
            lv2:minimum 0;
            lv2:name "Mode";
            lv2:portProperty lv2:enumeration,
                lv2:integer;
            lv2:scalePoint [ rdfs:label "bypass";
                    rdf:value 0 ],
                [ rdfs:label "atan";
                    rdf:value 1 ],
                [ rdfs:label "atan15";
                    rdf:value 2 ],
                [ rdfs:label "clip";
                    rdf:value 3 ],
                [ rdfs:label "one5";
                    rdf:value 4 ],
                [ rdfs:label "one53";
                    rdf:value 5 ],
                [ rdfs:label "clip3";
                    rdf:value 6 ],
                [ rdfs:label "clip9";
                    rdf:value 7 ],
                [ rdfs:label "sin1";
                    rdf:value 8 ],
                [ rdfs:label "pow7";
                    rdf:value 9 ],
                [ rdfs:label "tanh";
                    rdf:value 10 ],
                [ rdfs:label "rectify";
                    rdf:value 11 ];
            lv2:symbol "mode" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            units:unit units:db;
            lv2:default 0;
            lv2:index 3;
            lv2:maximum 72;
            lv2:minimum -24;
            lv2:name "Gain";
            lv2:symbol "gain" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0;
            lv2:index 4;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Bias";
            lv2:symbol "bias" ];
    mod:gui [ mod:iconTemplate <modgui/Saturate.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/Saturate.png>;
            mod:templateData <modgui/Saturate.json>;
            mod:thumbnail <modgui/Saturate-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* Saturate" .

