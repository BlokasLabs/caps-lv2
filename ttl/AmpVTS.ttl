@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://portalmod.com/plugins/caps/AmpVTS> a lv2:Plugin,
        lv2:SimulatorPlugin;
    lv2:microVersion 5;
    lv2:minorVersion 0;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.750;
            lv2:index 7;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Treble";
            lv2:symbol "treble" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 8;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Attack";
            lv2:symbol "attack" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 9;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Squash";
            lv2:symbol "squash" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.750;
            lv2:index 10;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Low cut";
            lv2:symbol "low_cut" ],
        [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 11;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 12;
            lv2:name "out";
            lv2:symbol "out" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 2;
            lv2:index 0;
            lv2:maximum 2;
            lv2:minimum 0;
            lv2:name "Over";
            lv2:portProperty lv2:integer;
            lv2:scalePoint [ rdfs:label "2x";
                    rdf:value 0 ],
                [ rdfs:label "4x";
                    rdf:value 1 ],
                [ rdfs:label "8x";
                    rdf:value 2 ];
            lv2:symbol "over" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 1;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Gain";
            lv2:symbol "gain" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Bright";
            lv2:symbol "bright" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 3;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Power";
            lv2:symbol "power" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 1;
            lv2:index 4;
            lv2:maximum 8;
            lv2:minimum 0;
            lv2:name "Tonestack";
            lv2:portProperty lv2:enumeration,
                lv2:integer;
            lv2:scalePoint [ rdfs:label "basswoman";
                    rdf:value 0 ],
                [ rdfs:label "twin";
                    rdf:value 1 ],
                [ rdfs:label "wookie";
                    rdf:value 2 ],
                [ rdfs:label "DC 30";
                    rdf:value 3 ],
                [ rdfs:label "juice 800";
                    rdf:value 4 ],
                [ rdfs:label "stanford";
                    rdf:value 5 ],
                [ rdfs:label "HK 20";
                    rdf:value 6 ],
                [ rdfs:label "nihon ace";
                    rdf:value 7 ],
                [ rdfs:label "porky";
                    rdf:value 8 ];
            lv2:symbol "tonestack" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 5;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Bass";
            lv2:symbol "bass" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 1;
            lv2:index 6;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Mid";
            lv2:symbol "mid" ];
    mod:gui [ mod:iconTemplate <modgui/AmpVTS.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/AmpVTS.png>;
            mod:templateData <modgui/AmpVTS.json>;
            mod:thumbnail <modgui/AmpVTS-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* AmpVTS - Tube amp + Tone stack" .

