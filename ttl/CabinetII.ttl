@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/CabinetII> a lv2:Plugin, lv2:SimulatorPlugin;
    doap:name  "C* CabinetII - Simplistic loudspeaker cabinet emulation";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 9;
    lv2:microVersion 16;

    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "model";
        lv2:name "Model";
    lv2:portProperty lv2:integer;
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 7;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "none"; rdf:value 0];
        lv2:scalePoint [rdfs:label "Unmatched A"; rdf:value 1];
        lv2:scalePoint [rdfs:label "Unmatched B"; rdf:value 2];
        lv2:scalePoint [rdfs:label "Supertramp"; rdf:value 3];
        lv2:scalePoint [rdfs:label "Little Wing 68"; rdf:value 4];
        lv2:scalePoint [rdfs:label "Martial"; rdf:value 5];
        lv2:scalePoint [rdfs:label "Mega Wookie"; rdf:value 6];
        lv2:scalePoint [rdfs:label "Pro Sr"; rdf:value 7];
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "gain";
        lv2:name "Gain";
        lv2:default 0;
        lv2:minimum -24;
        lv2:maximum 12;
    units:unit units:db ;

    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 3;
        lv2:symbol "out";
        lv2:name "out";
    ].
