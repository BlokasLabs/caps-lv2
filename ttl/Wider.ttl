@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .

<http://portalmod.com/plugins/caps/Wider> a lv2:DelayPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.0;
            lv2:index 1;
            lv2:maximum 1;
            lv2:minimum -1;
            lv2:name "pan";
            lv2:symbol "pan" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.5;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "width";
            lv2:symbol "width" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 3;
            lv2:name "out:l";
            lv2:symbol "outl" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 4;
            lv2:name "out:r";
            lv2:symbol "outr" ];
    mod:gui [ mod:iconTemplate <modgui/Wider.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/Wider.png>;
            mod:templateData <modgui/Wider.json>;
            mod:thumbnail <modgui/Wider-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* Wider - Stereo image Synthesis" .

