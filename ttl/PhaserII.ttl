@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://moddevices.com/plugins/caps/PhaserII> a lv2:PhaserPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;

rdfs:comment """
This take on the classic effect features two modulation choices, traditional sine-based periodicity or smoothened fractal oscillation. 

Very high resonance settings can cause self-oscillation peaking in excess of 0 dB. 

source: http://quitte.de/dsp/caps.html#PhaserII
""";

    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 0;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "rate";
            lv2:symbol "rate" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0;
            lv2:index 1;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "LFO";
            lv2:portProperty lv2:enumeration,
                lv2:integer;
            lv2:scalePoint [ rdfs:label "sine";
                    rdf:value 0 ],
                [ rdfs:label "fractal";
                    rdf:value 1 ];
            lv2:symbol "lfo" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 1;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "depth";
            lv2:symbol "depth" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.750;
            lv2:index 3;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "spread";
            lv2:symbol "spread" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 4;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "resonance";
            lv2:symbol "resonance" ],
        [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 5;
            lv2:name "in";
            lv2:symbol "in" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 6;
            lv2:name "out";
            lv2:symbol "out" ];
    modgui:gui [ modgui:iconTemplate <modgui/PhaserII.html>;
            modgui:resourcesDirectory <modgui>;
            modgui:screenshot <modgui/PhaserII.png>;
            modgui:stylesheet <modgui/_common.css>;
            modgui:templateData <modgui/PhaserII.json>;
            modgui:thumbnail <modgui/PhaserII-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://moddevices.com>;
            foaf:mbox <mailto:devel@moddevices.com>;
            foaf:name "MOD Team" ];
    doap:name "C* PhaserII - Mono phaser modulated by a Lorenz fractal" .

