@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://portalmod.com/plugins/caps/Narrower> a lv2:SpatialPlugin,
        lv2:Plugin;
    lv2:microVersion 17;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:port [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 0;
            lv2:name "in left";
            lv2:symbol "inl" ],
        [ a lv2:AudioPort,
                lv2:InputPort;
            lv2:index 1;
            lv2:name "in right";
            lv2:symbol "inr" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Mode";
            lv2:portProperty lv2:enumeration,
                lv2:integer;
            lv2:scalePoint [ rdfs:label "crossfeed mixing";
                    rdf:value 0 ],
                [ rdfs:label "mid/side processing";
                    rdf:value 1 ];
            lv2:symbol "mode" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.250;
            lv2:index 3;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "Strength";
            lv2:symbol "strength" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 4;
            lv2:name "out left";
            lv2:symbol "outl" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 5;
            lv2:name "out right";
            lv2:symbol "outr" ];
    mod:gui [ mod:iconTemplate <modgui/Narrower.html>;
            mod:resourcesDirectory <modgui>;
            mod:screenshot <modgui/Narrower.png>;
            mod:templateData <modgui/Narrower.json>;
            mod:thumbnail <modgui/Narrower-thumb.png> ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://portalmod.com>;
            foaf:mbox <mailto:devel@portalmod.com>;
            foaf:name "MOD Team" ];
    doap:name "C* Narrower - Stereo image width reduction" .

