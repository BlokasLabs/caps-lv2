@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .


<http://portalmod.com/plugins/caps/Sin> a lv2:Plugin, lv2:OscillatorPlugin;
    doap:name  "C* Sin - Sine wave generator";
    mod:label "C* Sin";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;

    mod:pedalModel "japanese";
    mod:pedalColor "green";
    mod:brand "caps";
    mod:pedalLabel "Sin";
    mod:smallLabel "Sin";
    mod:bypassLedPosition "118-83";

    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "f";
        lv2:name "f";
        units:unit units:hz ;
        lv2:portProperty epp:logarithmic;
        lv2:portProperty pprops:logarithmic ;
        lv2:default 100.0;
        lv2:minimum 0.1;
        lv2:maximum 20000;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "volume";
        lv2:name "volume";
        lv2:default 0.5000005;
        lv2:minimum 0.000001;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 2;
        lv2:symbol "out";
        lv2:name "out";
    ].
