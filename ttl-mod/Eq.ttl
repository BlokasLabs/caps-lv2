@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/caps/Eq> a lv2:Plugin, lv2:EQPlugin;
    doap:name  "C* Eq - 10-band equalizer";
    mod:label "C* Eq";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "band31hz";
        lv2:name "31 Hz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "band63hz";
        lv2:name "63 Hz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "band125hz";
        lv2:name "125 Hz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "band250hz";
        lv2:name "250 Hz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "band500hz";
        lv2:name "500 Hz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "band1khz";
        lv2:name "1 kHz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "band2khz";
        lv2:name "2 kHz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 8;
        lv2:symbol "band4khz";
        lv2:name "4 kHz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 9;
        lv2:symbol "band8khz";
        lv2:name "8 kHz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 10;
        lv2:symbol "band16khz";
        lv2:name "16 kHz";
        lv2:default 0.0;
        lv2:minimum -48;
        lv2:maximum 24;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 11;
        lv2:symbol "out";
        lv2:name "out";
    ].
