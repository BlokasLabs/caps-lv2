@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .


<http://portalmod.com/plugins/caps/Compress> a lv2:Plugin, lv2:CompressorPlugin;
    doap:name  "C* Compress - Mono compressor";
    mod:label "C* Compress";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 2;

        mod:pedalModel "japanese";
    mod:pedalColor "red";
    mod:brand "caps";
    mod:pedalLabel "Compress";
    mod:smallLabel "Compress";
    mod:bypassLedPosition "118-83";

    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "measure";
        lv2:name "Measure";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
        lv2:portProperty lv2:integer;
        lv2:scalePoint [rdfs:label "peak"; rdf:value 0];
        lv2:scalePoint [rdfs:label "rms"; rdf:value 1];
    mod:iconLabel "Measure";
    mod:pedalButton "0-5";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "mode";
        lv2:name "Mode";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 3;
        lv2:portProperty lv2:integer;
        lv2:scalePoint [rdfs:label "linear"; rdf:value 0];
        lv2:scalePoint [rdfs:label "saturating 2x"; rdf:value 1];
        lv2:scalePoint [rdfs:label "saturating 4x"; rdf:value 2];
        lv2:scalePoint [rdfs:label "saturating 4x128"; rdf:value 3];
    mod:iconLabel "Mode";
    mod:pedalButton "0-5";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "threshold";
        lv2:name "Threshold";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
    mod:iconLabel "Threshold";
    mod:pedalButton "2-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "strenght";
        lv2:name "Strenght";
        lv2:default 0.25;
        lv2:minimum 0;
        lv2:maximum 1;
    mod:iconLabel "Strenght";
    mod:pedalButton "2-6";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "attack";
        lv2:name "Attack";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
    mod:iconLabel "Attack";
    mod:pedalButton "2-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "release";
        lv2:name "release";
        lv2:default 0.25;
        lv2:minimum 0;
        lv2:maximum 1;
    mod:iconLabel "release";
    mod:pedalButton "2-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "gain";
        lv2:name "gain";
        lv2:default 0;
        lv2:minimum -12;
        lv2:maximum 24;
        units:unit units:db ;
    mod:iconLabel "gain";
    mod:pedalButton "0-1";
    ],
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 8;
        lv2:symbol "out";
        lv2:name "out";
    ].
