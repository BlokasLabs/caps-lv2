@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/AmpIII> a lv2:Plugin, lv2:SimulatorPlugin;
    doap:name  "C* AmpIII - Tube amp";
    mod:label "C* AmpIII";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    
    	mod:pedalModel "japanese";
	mod:pedalColor "brown";
	mod:brand "caps";
	mod:pedalLabel "AmpIII";
	mod:smallLabel "AmpIII";
	mod:bypassLedPosition "118-83";
    
    lv2:minorVersion 0;
    lv2:microVersion 4;
    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "gain";
        lv2:name "Gain";
        lv2:default 2.5;
        lv2:minimum 0;
        lv2:maximum 10;
        units:unit units:db ;
	mod:iconLabel "Gain";
	mod:pedalButton "1-1";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "temperature";
        lv2:name "Temperature";
        lv2:default 0.5025;
        lv2:minimum 0.005;
        lv2:maximum 1;
        mod:iconLabel "Temp";
	mod:pedalButton "1-3";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "drive";
        lv2:name "Drive";
        lv2:default 1;
        lv2:minimum 0.0001;
        lv2:maximum 1;
        mod:iconLabel "Drive";
	mod:pedalButton "1-5";

    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 4;
        lv2:symbol "out";
        lv2:name "out";
    ],
    [
        a lv2:ControlPort, lv2:OutputPort;
        lv2:index 5;
        lv2:symbol "latency";
        lv2:name "latency";
        lv2:default 1;
        lv2:minimum 0.0001;
        lv2:maximum 1;
    ].
