@prefix doap:  <http://usefulinc.com/ns/doap#>.
@prefix epp:   <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf:  <http://xmlns.com/foaf/0.1/>.
@prefix lv2:   <http://lv2plug.in/ns/lv2core#>.
@prefix mod:   <http://moddevices.com/ns/mod#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#>.

<http://moddevices.com/plugins/caps/CabinetIII>
a lv2:SimulatorPlugin, lv2:Plugin;
lv2:optionalFeature lv2:hardRtCapable;
lv2:microVersion 24;
lv2:minorVersion 9;
mod:brand "CAPS";
mod:label "CabinetIII";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:name "in";
    lv2:symbol "in";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 2;
    lv2:name "Model";
    lv2:symbol "model";
    lv2:portProperty lv2:enumeration, lv2:integer;
    lv2:scalePoint
    [
        rdfs:label "Wookie A";
        rdf:value 0;
    ],
    [
        rdfs:label "Wookie B";
        rdf:value 1;
    ],
    [
        rdfs:label "Wookie C";
        rdf:value 2;
    ];
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:default 0.0;
    lv2:minimum -24.0;
    lv2:maximum 12.0;
    lv2:name "Gain";
    lv2:symbol "gain";
    units:unit units:db ;
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 3;
    lv2:name "out";
    lv2:symbol "out";
];

doap:license <http://usefulinc.com/doap/licenses/gpl>;

doap:developer [
    foaf:homepage <http://quitte.de/dsp/caps.html>;
    foaf:mbox <mailto:tim@quitte.de>;
    foaf:name "Tim Goetze";
];

doap:maintainer [
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    foaf:name "MOD Team";
];

doap:name "C* CabinetIII - Idealised loudspeaker cabinet emulation".
