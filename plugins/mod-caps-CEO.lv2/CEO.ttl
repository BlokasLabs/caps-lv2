@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://moddevices.com/plugins/caps/CEO> a lv2:OscillatorPlugin,
        lv2:Plugin;
    lv2:microVersion 16;
    lv2:minorVersion 9;
    lv2:optionalFeature lv2:hardRtCapable;
    mod:brand "CAPS";
    mod:label "CEO";

rdfs:comment """
The Chief Executive Oscillator forever calls for more profit. 

Sound data created with the flite[flite] application. 

source: http://quitte.de/dsp/caps.html#CEO
""";

    lv2:port [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 184.0;
            lv2:index 0;
            lv2:maximum 244;
            lv2:minimum 4;
            lv2:name "Profit per minute";
            lv2:symbol "ppm" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0.50;
            lv2:index 1;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "volume";
            lv2:symbol "volume" ],
        [ a lv2:ControlPort,
                lv2:InputPort;
            lv2:default 0;
            lv2:index 2;
            lv2:maximum 1;
            lv2:minimum 0;
            lv2:name "damping";
            lv2:symbol "damping" ],
        [ a lv2:AudioPort,
                lv2:OutputPort;
            lv2:index 3;
            lv2:name "out";
            lv2:symbol "out" ];
    doap:developer [ foaf:homepage <http://quitte.de/dsp/caps.html>;
            foaf:mbox <mailto:tim@quitte.de>;
            foaf:name "Tim Goetze" ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    doap:maintainer [ foaf:homepage <http://moddevices.com>;
            foaf:mbox <mailto:devel@moddevices.com>;
            foaf:name "MOD Team" ];
    doap:name "C* CEO - Chief Executive Oscillator" .

