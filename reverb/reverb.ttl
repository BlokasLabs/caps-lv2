@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/JVRev> a lv2:Plugin, lv2:ReverbPlugin;
    doap:name  "C* JVRev - Stanford-style reverb from STK";
    mod:label "C* JVRev";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 2;

    mod:pedalModel "japanese";
    mod:pedalColor "cream";
    mod:brand "caps";
    mod:pedalLabel "JV Reverb";
    mod:smallLabel "JVRev";
    mod:bypassLedPosition "118-83";


    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "t60s";
        lv2:name "t60";
        lv2:default 2.3;
        lv2:minimum 0;
        lv2:maximum 4.6;
    	units:unit units:s ;
	mod:pedalButton "1-1";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "blend";
        lv2:name "blend";
        lv2:default 0.14;
        lv2:minimum 0;
        lv2:maximum 0.28;
	mod:pedalButton "1-5";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 3;
        lv2:symbol "outl";
        lv2:name "out:l";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 4;
        lv2:symbol "outr";
        lv2:name "out:r";
    ].
