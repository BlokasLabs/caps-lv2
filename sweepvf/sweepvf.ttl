@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .


<http://portalmod.com/plugins/caps/SweepVFI> a lv2:Plugin, lv2:FilterPlugin;
    doap:name  "C* SweepVFI - Resonant filter swept by a Lorenz fractal";
    mod:label "C* SweepVFI";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 2;

        mod:pedalModel "japanese";
	mod:pedalColor "yellow";
	mod:brand "caps";
	mod:pedalLabel "SweepVF I";
	mod:smallLabel "SweepVFI";
	mod:bypassLedPosition "118-83";


    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "frequency";
        lv2:name "freq";
        lv2:portProperty epp:logarithmic;
        lv2:default 529.895272672;
        lv2:minimum 83;
        lv2:maximum 3383;
        units:unit units:hz ;
	mod:iconLabel "freq";
	mod:pedalButton "0-1";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "q";
        lv2:name "Q";
        lv2:default 0.5;
        lv2:minimum 0.001;
        lv2:maximum 0.999;
        mod:iconLabel "Q";
	mod:pedalButton "0-3";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "mode";
        lv2:name "mode";
        lv2:default 1.0;
        lv2:minimum 0;
        lv2:maximum 1;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "Low pass"; rdf:value 0];
        lv2:scalePoint [rdfs:label "Band pass"; rdf:value 1];
	mod:iconLabel "mode";
	mod:pedalButton "0-5";



    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "depthx";
        lv2:name "depth:x";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
	mod:iconLabel "x";
	mod:pedalButton "2-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "depthy";
        lv2:name "depth:y";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
	mod:iconLabel "y";
	mod:pedalButton "2-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "depthz";
        lv2:name "depth:z";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
	mod:iconLabel "z";
	mod:pedalButton "2-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "h";
        lv2:name "h";
        lv2:default 0.5005;
        lv2:minimum 0.001;
        lv2:maximum 1;
	mod:iconLabel "h";
	mod:pedalButton "2-6";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 8;
        lv2:symbol "out";
        lv2:name "out";
    ].













