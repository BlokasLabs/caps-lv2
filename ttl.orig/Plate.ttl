@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<http://quitte.de/dsp/caps.html#Plate> a lv2:Plugin, lv2:ReverbPlugin;
    doap:name  "C* Plate - Versatile plate reverb";

    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];

    doap:maintainer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];

    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;

    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "bandwidth";
        lv2:name "bandwidth";
        lv2:default 0.9995;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "tail";
        lv2:name "tail";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "damping";
        lv2:name "damping";
        lv2:default 0.0005;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "blend";
        lv2:name "blend";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 5;
        lv2:symbol "outl";
        lv2:name "out:l";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 6;
        lv2:symbol "outr";
        lv2:name "out:r";
    ].
