@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/caps/VCOs> a lv2:Plugin, lv2:OscillatorPlugin;
    doap:name  "C* VCOs - Virtual 'analogue' oscillator";
    mod:label "C* VCOs";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "f";
        lv2:name "f";
    lv2:portProperty epp:logarithmic;
        lv2:default 100.0;
        lv2:minimum 1;
        lv2:maximum 5751;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "trisaw";
        lv2:name "tri .. saw";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "square";
        lv2:name "~ .. square";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "volume";
        lv2:name "volume";
        lv2:default 0.5000005;
        lv2:minimum 0.000001;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 4;
        lv2:symbol "out";
        lv2:name "out";
    ].
<http://portalmod.com/plugins/caps/VCOd> a lv2:Plugin, lv2:OscillatorPlugin;
    doap:name  "C* VCOd - Double VCO with detune and hard sync options";
    mod:label "C* VCOd";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "f";
        lv2:name "f";
    lv2:portProperty epp:logarithmic;
        lv2:default 100.0;
        lv2:minimum 1;
        lv2:maximum 5751;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "1trisaw";
        lv2:name "1: tri .. saw";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "1square";
        lv2:name "1: ~ .. square";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "2trisaw";
        lv2:name "2: tri .. saw";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "2square";
        lv2:name "2: ~ .. square";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "2tune";
        lv2:name "2: tune";
        lv2:default 0.0;
        lv2:minimum -12;
        lv2:maximum 12;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "sync";
        lv2:name "sync";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "blend";
        lv2:name "blend";
        lv2:default 0.5;
        lv2:minimum -1;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 8;
        lv2:symbol "volume";
        lv2:name "volume";
        lv2:default 0.5000005;
        lv2:minimum 0.000001;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 9;
        lv2:symbol "out";
        lv2:name "out";
    ].
