@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .

<http://portalmod.com/plugins/caps/ampvts> a lv2:Plugin, lv2:SimulatorPlugin;
    doap:name  "C* AmpVTS - Tube amp + Tone stack";
    mod:label "C* AmpVTS";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;

    mod:pedalModel "japanese";
    mod:pedalColor "brown";
    mod:brand "C*";
    mod:pedalLabel "AmpVTS";
    mod:smallLabel "AmpVTS";
    mod:bypassLedPosition "118-83";

    lv2:minorVersion 0;
    lv2:microVersion 4;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "ratio";
        lv2:name "Ratio";
        lv2:default 2;
        lv2:minimum 0;
        lv2:maximum 2;
        lv2:portProperty lv2:integer;
        lv2:scalePoint [rdfs:label "2x"; rdf:value 0];
        lv2:scalePoint [rdfs:label "4x"; rdf:value 1];
        lv2:scalePoint [rdfs:label "8x"; rdf:value 2];
        mod:iconLabel "Ratio";
        mod:pedalButton "0-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "model";
        lv2:name "Model";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 8;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "basswoman"; rdf:value 0];
        lv2:scalePoint [rdfs:label "wookie"; rdf:value 1];
        lv2:scalePoint [rdfs:label "DC 30"; rdf:value 2];
        lv2:scalePoint [rdfs:label "twin"; rdf:value 3];
        lv2:scalePoint [rdfs:label "stanford"; rdf:value 4];
        lv2:scalePoint [rdfs:label "juice 800"; rdf:value 5];
        lv2:scalePoint [rdfs:label "HK 20"; rdf:value 6];
        lv2:scalePoint [rdfs:label "porky"; rdf:value 7];
        lv2:scalePoint [rdfs:label "nihon ace"; rdf:value 8];
        mod:iconLabel "Model";
        mod:pedalButton "0-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "gain";
        lv2:name "Gain";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Gain";
        mod:pedalButton "0-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "bright";
        lv2:name "Bright";
        lv2:default 0;
        lv2:minimum 0.5;
        lv2:maximum 1;
        mod:iconLabel "Bright";
        mod:pedalButton "0-6";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "power";
        lv2:name "Power";
        lv2:default 0;
        lv2:minimum 0.5;
        lv2:maximum 1;
        mod:iconLabel "Power";
        mod:pedalButton "2-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "bass";
        lv2:name "Bass";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Bass";
        mod:pedalButton "2-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "mid";
        lv2:name "Mid";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Mid";
        mod:pedalButton "2-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "treble";
        lv2:name "Treble";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Treble";
        mod:pedalButton "2-6";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 8;
        lv2:symbol "attack";
        lv2:name "Attack";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 9;
        lv2:symbol "squash";
        lv2:name "Squash";
        lv2:default 0;
        lv2:minimum 0.5;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 10;
        lv2:symbol "low_cut";
        lv2:name "Low cut";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 11;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 12;
        lv2:symbol "out";
        lv2:name "out";
    ].
