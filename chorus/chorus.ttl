@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/caps/ChorusI> a lv2:Plugin, lv2:ChorusPlugin;
    doap:name  "C* ChorusI - Mono chorus/flanger";
    mod:label "C* ChorusI";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "tms";
        lv2:name "t (ms)";
    lv2:portProperty epp:logarithmic;
        lv2:default 10.0;
        lv2:minimum 2.5;
        lv2:maximum 40;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "widthms";
        lv2:name "width (ms)";
        lv2:default 1.0;
        lv2:minimum .5;
        lv2:maximum 10;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "ratehz";
        lv2:name "rate (Hz)";
        lv2:default 2.5;
        lv2:minimum 0;
        lv2:maximum 5;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "blend";
        lv2:name "blend";
        lv2:default 1.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "feedforward";
        lv2:name "feedforward";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "feedback";
        lv2:name "feedback";
        lv2:default 0.0;
        lv2:minimum 0;
        lv2:maximum 1;
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 7;
        lv2:symbol "out";
        lv2:name "out";
    ].
