@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<http://portalmod.com/plugins/caps/noisegate> a lv2:Plugin, lv2:FilterPlugin;
    doap:name  "C* Noisegate";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;

    mod:pedalModel "japanese";
    mod:pedalColor "yellow";
    mod:brand "C*";
    mod:pedalLabel "Noisegate";
    mod:smallLabel "Noisegate";
    mod:bypassLedPosition "118-83";

    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 1;
        lv2:symbol "out";
        lv2:name "out";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "open";
        lv2:name "Open";
        lv2:default -40;
        lv2:minimum -60;
        lv2:maximum 0;
        units:unit units:db;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "close";
        lv2:name "Close";
        lv2:default -40;
        lv2:minimum -90;
        lv2:maximum 0;
        units:unit units:db;
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "mains";
        lv2:name "Mains";
        lv2:default 50;
        lv2:minimum 0;
        lv2:maximum 100;
        units:unit units:hz;
    ].
