@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

<http://portalmod.com/plugins/caps/autofilter> a lv2:Plugin, lv2:FilterPlugin;
    doap:name  "C* AutoFilter";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;

    mod:pedalModel "japanese";
    mod:pedalColor "yellow";
    mod:brand "C*";
    mod:pedalLabel "AutoFilter";
    mod:smallLabel "AutoFilter";
    mod:bypassLedPosition "118-83";

    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "over";
        lv2:name "Over";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 3;
        lv2:portProperty lv2:integer;
        lv2:scalePoint [rdfs:label "nome"; rdf:value 0];
        lv2:scalePoint [rdfs:label "2x"; rdf:value 1];
        lv2:scalePoint [rdfs:label "4x"; rdf:value 2];
        lv2:scalePoint [rdfs:label "8x"; rdf:value 3];
        mod:iconLabel "Over";
        mod:pedalButton "0-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "mode";
        lv2:name "Mode";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        lv2:portProperty lv2:integer;
        lv2:scalePoint [rdfs:label "low pass"; rdf:value 0];
        lv2:scalePoint [rdfs:label "band pass"; rdf:value 1];
        mod:iconLabel "Mode";
        mod:pedalButton "0-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "filter";
        lv2:name "Filter";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 4;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "breathy"; rdf:value 0];
        lv2:scalePoint [rdfs:label "fat A"; rdf:value 1];
        lv2:scalePoint [rdfs:label "fat B"; rdf:value 2];
        lv2:scalePoint [rdfs:label "fat C"; rdf:value 3];
        lv2:scalePoint [rdfs:label "fat D"; rdf:value 4];
        mod:iconLabel "Model";
        mod:pedalButton "0-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "gain";
        lv2:name "Gain";
        lv2:default 6;
        lv2:minimum 0;
        lv2:maximum 24;
        units:unit units:db ;
        mod:iconLabel "Gain";
        mod:pedalButton "0-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "frequency";
        lv2:name "Frequency";
        lv2:default 968;
        lv2:minimum 43;
        lv2:maximum 3743;
        lv2:portProperty epp:logarithmic;
        lv2:portProperty pprops:logarithmic ;
        units:unit units:hz ;
        mod:iconLabel "Freq";
        mod:pedalButton "0-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 5;
        lv2:symbol "Q";
        lv2:name "Q";
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Q";
        mod:pedalButton "0-6";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 6;
        lv2:symbol "range";
        lv2:name "Range";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Range";
        mod:pedalButton "2-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 7;
        lv2:symbol "lfoenv";
        lv2:name "Low Frequency Oscillator/Envelope";
        lv2:default 0.25;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "lfo/env";
        mod:pedalButton "2-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 8;
        lv2:symbol "rate";
        lv2:name "Rate";
        lv2:default 0.25;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "Rate";
        mod:pedalButton "2-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 9;
        lv2:symbol "xz";
        lv2:name "X/Z";
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        mod:iconLabel "x/z";
        mod:pedalButton "2-6";
    ],
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 10;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 11;
        lv2:symbol "out";
        lv2:name "out";
    ].
