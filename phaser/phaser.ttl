@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://portalmod.com/plugins/caps/PhaserI> a lv2:Plugin, lv2:PhaserPlugin;
    doap:name  "C* PhaserI - Mono phaser";
    mod:label "C* PhaserI";
    doap:developer [
        foaf:name "Tim Goetze";
        foaf:homepage <http://quitte.de/dsp/caps.html>;
        foaf:mbox <mailto:tim@quitte.de>;
        ];
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://portalmod.com>;
        foaf:mbox <mailto:devel@portalmod.com>;
        ];
    doap:license <http://usefulinc.com/doap/licenses/gpl>;
    lv2:optionalFeature lv2:hardRtCapable;
    lv2:minorVersion 0;
    lv2:microVersion 3;

    mod:pedalModel "japanese";
    mod:pedalColor "purple";
    mod:brand "caps";
    mod:pedalLabel "Phaser I";
    mod:smallLabel "PhaserI";
    mod:bypassLedPosition "118-83";

    lv2:port
    [
        a lv2:AudioPort, lv2:InputPort;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "in";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 1;
        lv2:symbol "rate";
        lv2:name "rate";
        lv2:default 0.5;
        lv2:minimum 0;
        lv2:maximum 10;
 	units:unit units:hz ;
	mod:pedalButton "1-0";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 2;
        lv2:symbol "depth";
        lv2:name "depth";
        lv2:default 0.65;
        lv2:minimum 0;
        lv2:maximum 1;
	mod:pedalButton "1-2";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 3;
        lv2:symbol "spread";
        lv2:name "spread";
        lv2:default 1.57079632679;
        lv2:minimum 0;
        lv2:maximum 3.14159265358979323846;
	mod:pedalButton "1-4";
    ],
    [
        a lv2:ControlPort, lv2:InputPort;
        lv2:index 4;
        lv2:symbol "feedback";
        lv2:name "feedback";
        lv2:default 0.65;
        lv2:minimum 0;
        lv2:maximum 0.999;
	mod:pedalButton "1-6";
    ],
    [
        a lv2:AudioPort, lv2:OutputPort;
        lv2:index 5;
        lv2:symbol "out";
        lv2:name "out";
    ].
